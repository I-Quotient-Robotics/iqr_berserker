<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find berserker_navigation)/config/move_base_config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find berserker_navigation)/config/move_base_config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find berserker_navigation)/config/move_base_config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find berserker_navigation)/config/move_base_config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find berserker_navigation)/config/move_base_config/base_local_planner_params.yaml" command="load" />
  </node>
  
</launch>
